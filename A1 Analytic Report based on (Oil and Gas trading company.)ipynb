    "# Step 1: Group wells based on common characteristics\n",
    "grouped_wells = merged_data.groupby(['ENVWellType', 'CompletionID'])\n",
    "\n",
    "# Step 2: Calculate average production rates over time for each group\n",
    "average_production = grouped_wells[['ProducingMonth', 'Production_BOE']].mean().reset_index()\n",
    "\n",
    "# Step 3: Visualize average production for each group (limiting to the first 5 groups for example)\n",
    "limit_groups = 5\n",
    "for i, (group_key, group_data) in enumerate(average_production.groupby(['ENVWellType', 'CompletionID'])):\n",
    "    if i >= limit_groups:\n",
    "        break\n",
    "    plt.bar(f'{group_data.iloc[0][\"ENVWellType\"]} - {group_data.iloc[0][\"CompletionID\"]}', \n",
    "            group_data['Production_BOE'].mean(), label=f'{group_data.iloc[0][\"ENVWellType\"]} - {group_data.iloc[0][\"CompletionID\"]}',)\n",
    "\n",
    "plt.xlabel('Well Groups')\n",
    "plt.ylabel('Average Production (BOE)')\n",
    "plt.xticks(rotation=15, ha='right')\n",
    "plt.tight_layout()  # Adjust layout for better spacing\n",
    "plt.title(f'Average Production for Different Well Types and Completion Methods (Limited to {limit_groups} groups)')\n",
    "plt.show()"
   ]
