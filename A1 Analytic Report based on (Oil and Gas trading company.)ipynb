   ],
   "source": [
    "# Step 2: Identify wells with production rates above a predefined threshold\n",
    "# Assuming the threshold is defined as the 75th percentile of production for both BOE and MCFE\n",
    "threshold_boe = well_production['Production_BOE'].quantile(0.75)\n",
    "threshold_mcfe = well_production['Production_MCFE'].quantile(0.75)\n",
    "\n",
    "high_performing_wells_boe = well_production[well_production['Production_BOE'] > threshold_boe]\n",
    "high_performing_wells_mcfe = well_production[well_production['Production_MCFE'] > threshold_mcfe]\n",
    "\n",
    "# Step 3: Rank wells based on historical performance\n",
    "# Combine both high-performing wells based on BOE and MCFE\n",
    "high_performing_wells_combined = pd.concat([high_performing_wells_boe, high_performing_wells_mcfe], ignore_index=True)\n",
    "\n",
    "# Sort wells based on average production rates\n",
    "high_performing_wells_combined.sort_values(by=['Production_BOE', 'Production_MCFE'], ascending=False, inplace=True)\n",
    "\n",
    "# Display the top high-performing wells\n",
    "print(\"Top High-Performing Wells:\")\n",
    "print(high_performing_wells_combined.head(10))\n",
    "\n",
    "# Visualize the distribution of production rates\n",
    "print(\"Visualize the distribution of production rates\")\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.hist(well_production['Production_BOE'], bins=20, alpha=0.5, color='blue', label='BOE Production')\n",
    "plt.hist(well_production['Production_MCFE'], bins=20, alpha=0.5, color='grey', label='MCFE Production')\n",
    "plt.xlabel('Average Production Rate')\n",
    "plt.ylabel('Frequency')\n",
    "plt.title('Distribution of Production Rates')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  },
